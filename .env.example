# PMIP Environment Configuration
# Copy this file to .env and fill in your actual values

# Environment
NODE_ENV=development
LOG_LEVEL=info
PORT=3000

# PropertyWare Configuration
PW_URL=https://api.propertyware.com
PW_WSDL=https://api.propertyware.com/services?wsdl
PW_USERNAME=your-propertyware-username
PW_PASSWORD=your-propertyware-password

# ServiceFusion Configuration
SF_BASE_URL=https://api.servicefusion.com
SF_CLIENT_ID=your-servicefusion-client-id
SF_CLIENT_SECRET=your-servicefusion-client-secret

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE=your-service-role-key

# AWS Configuration (for GreenLight integration)
AWS_REGION=us-east-1
AWS_PROFILE=greenlight-dev

# Lambda Function ARNs (if using GreenLight)
LAMBDA_WORKORDERS=arn:aws:lambda:us-east-1:557477747490:function:GreenLightWorkOrders
LAMBDA_LEASES=arn:aws:lambda:us-east-1:557477747490:function:GreenLightLeases
LAMBDA_TENANTS=arn:aws:lambda:us-east-1:557477747490:function:GreenLightTenants
LAMBDA_WEB=arn:aws:lambda:us-east-1:557477747490:function:GreenLightWebTrigger
SNS_TOPIC=arn:aws:sns:us-east-1:557477747490:GreenLightSNSTopic

# Redis Configuration (optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Rate Limiting
RATE_LIMIT_PW=2
RATE_LIMIT_SF=0.5

# Deduplication
DEDUP_CONFIDENCE=0.95
DEDUP_CACHE_TTL=3600

# Monitoring (optional)
MONITORING_PROVIDER=cloudwatch
MONITORING_API_KEY=

# Feature Flags
DRY_RUN=false
ENABLE_CACHE=true
ENABLE_DEDUPLICATION=true
ENABLE_MONITORING=true